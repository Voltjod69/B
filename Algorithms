\documentclass{article} \usepackage[ruled,linesnumbered]{algorithm2e} \begin{document}
\begin{algorithm}
\caption{Euclid's algorithm}\label{euclid} \KwData{Two integers $a$ and $b$} \KwResult{GCD of $a$ and $b$} \eIf{$b=0$}{
return $a$\; }{
return Euclid$(b, a \bmod b)$\; }
\end{algorithm} \end{document}
    
